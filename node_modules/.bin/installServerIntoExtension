#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/leonzalion/projects/jslatex/jslatex-vscode-extension/node_modules/.pnpm/vscode-languageserver@6.1.1/node_modules/vscode-languageserver/bin/node_modules:/Users/leonzalion/projects/jslatex/jslatex-vscode-extension/node_modules/.pnpm/vscode-languageserver@6.1.1/node_modules/vscode-languageserver/node_modules:/Users/leonzalion/projects/jslatex/jslatex-vscode-extension/node_modules/.pnpm/vscode-languageserver@6.1.1/node_modules:/Users/leonzalion/projects/jslatex/jslatex-vscode-extension/node_modules/.pnpm/node_modules:/Users/leonzalion/projects/jslatex/jslatex-vscode-extension/node_modules:/Users/leonzalion/projects/jslatex/node_modules:/Users/leonzalion/projects/node_modules:/Users/leonzalion/node_modules:/Users/node_modules:/node_modules:/Users/leonzalion/projects/jslatex/jslatex-vscode-extension/node_modules/vscode-languageserver/bin/node_modules:/Users/leonzalion/projects/jslatex/jslatex-vscode-extension/node_modules/vscode-languageserver/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/leonzalion/projects/jslatex/jslatex-vscode-extension/node_modules/.pnpm/vscode-languageserver@6.1.1/node_modules/vscode-languageserver/bin/node_modules:/Users/leonzalion/projects/jslatex/jslatex-vscode-extension/node_modules/.pnpm/vscode-languageserver@6.1.1/node_modules/vscode-languageserver/node_modules:/Users/leonzalion/projects/jslatex/jslatex-vscode-extension/node_modules/.pnpm/vscode-languageserver@6.1.1/node_modules:/Users/leonzalion/projects/jslatex/jslatex-vscode-extension/node_modules/.pnpm/node_modules:/Users/leonzalion/projects/jslatex/jslatex-vscode-extension/node_modules:/Users/leonzalion/projects/jslatex/node_modules:/Users/leonzalion/projects/node_modules:/Users/leonzalion/node_modules:/Users/node_modules:/node_modules:/Users/leonzalion/projects/jslatex/jslatex-vscode-extension/node_modules/vscode-languageserver/bin/node_modules:/Users/leonzalion/projects/jslatex/jslatex-vscode-extension/node_modules/vscode-languageserver/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vscode-languageserver/bin/installServerIntoExtension" "$@"
else
  exec node  "$basedir/../vscode-languageserver/bin/installServerIntoExtension" "$@"
fi
