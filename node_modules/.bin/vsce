#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/leonzalion/projects/jslatex/node_modules/.pnpm/vsce@2.7.0/node_modules/vsce/node_modules:/Users/leonzalion/projects/jslatex/node_modules/.pnpm/vsce@2.7.0/node_modules:/Users/leonzalion/projects/jslatex/node_modules/.pnpm/node_modules:/Users/leonzalion/projects/jslatex/node_modules:/Users/leonzalion/projects/node_modules:/Users/leonzalion/node_modules:/Users/node_modules:/node_modules:/Users/leonzalion/projects/jslatex/packages/extension/node_modules:/Users/leonzalion/projects/jslatex/packages/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/leonzalion/projects/jslatex/node_modules/.pnpm/vsce@2.7.0/node_modules/vsce/node_modules:/Users/leonzalion/projects/jslatex/node_modules/.pnpm/vsce@2.7.0/node_modules:/Users/leonzalion/projects/jslatex/node_modules/.pnpm/node_modules:/Users/leonzalion/projects/jslatex/node_modules:/Users/leonzalion/projects/node_modules:/Users/leonzalion/node_modules:/Users/node_modules:/node_modules:/Users/leonzalion/projects/jslatex/packages/extension/node_modules:/Users/leonzalion/projects/jslatex/packages/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/vsce@2.7.0/node_modules/vsce/vsce" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/vsce@2.7.0/node_modules/vsce/vsce" "$@"
fi
